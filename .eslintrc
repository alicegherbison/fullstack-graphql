{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "project": "tsconfig.json"
  },
  "env": {
    "shared-node-browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "no-unused-vars": "warn",
    "no-undef": "off",
    "no-redeclare": "warn",
    "prefer-const": "warn"
  }
}
